{
  "name": "async-html-inline",
  "version": "1.3.2",
  "description": "Asynchronously inline javascript, stylesheets, images, video, and fonts to an html page.",
  "main": "dist/async-html-inline.js",
  "module": "dist/async-html-inline.mjs",
  "exports": {
    ".": {
      "require": "./dist/async-html-inline.js",
      "import": "./dist/async-html-inline.mjs",
      "types": "./dist/async-html-inline.d.ts"
    }
  },
  "types": "dist/async-html-inline.d.ts",
  "bin": {
    "@fwalzel/html-inline": "./bin/html-inline",
    "html-inline": "./bin/html-inline"
  },
  "publishConfig": {
    "access": "public"
  },
  "scripts": {
    "build": "node build.js",
    "prepublishOnly": "npm run build",
    "test": "npm run build && c8 mocha test/**/*.test.js",
    "test:watch": "mocha test/**/*.test.js --watch",
    "coverage": "npm run test && c8 report --reporter=text-lcov | coveralls",
    "example": "cd example && node ../bin/async-html-inline-cli.mjs input.html output.html"
  },
  "repository": {
    "type": "git",
    "url": "git+https://github.com/fwalzel/async-html-inline.git"
  },
  "keywords": [
    "html",
    "inline",
    "embed",
    "resources",
    "async await",
    "stylesheets",
    "scripts",
    "images",
    "video",
    "fonts",
    "assets",
    "base64",
    "typescript"
  ],
  "author": "Florian Walzel",
  "license": "MIT",
  "bugs": {
    "url": "https://github.com/fwalzel/async-html-inline/issues"
  },
  "homepage": "https://github.com/fwalzel/async-html-inline#readme",
  "dependencies": {
    "axios": "^1.13.5",
    "esm": "^3.2.25",
    "mime": "^4.1.0",
    "util": "^0.12.5"
  },
  "devDependencies": {
    "@types/node": "^25.2.3",
    "c8": "^10.1.3",
    "chai": "^6.2.2",
    "coveralls-next": "^6.0.1",
    "mocha": "^11.7.5",
    "typescript": "^5.9.3"
  }
}
