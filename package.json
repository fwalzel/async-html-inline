{
  "name": "async-html-inline",
  "version": "1.3.0",
  "description": "asynchronously inline javascript, stylesheets, and images to an html page",
  "main": "dist/async-html-inline.js",
  "module": "dist/async-html-inline.mjs",
  "exports": {
    ".": {
      "require": "./dist/async-html-inline.js",
      "import": "./dist/async-html-inline.mjs",
      "types": "./dist/async-html-inline.d.ts"
    }
  },
  "types": "dist/async-html-inline.d.ts",
  "bin": {
    "@fwalzel/html-inline": "./bin/html-inline",
    "html-inline": "./bin/html-inline"
  },
  "publishConfig": {
    "access": "public"
  },
  "scripts": {
    "build": "node build.js",
    "prepublishOnly": "npm run build",
    "test": "echo \"Error: no test specified\" && exit 1",
    "example": "cd example && node example-inline.js"
  },
  "repository": {
    "type": "git",
    "url": "git+https://github.com/fwalzel/async-html-inline.git"
  },
  "keywords": [
    "html",
    "inline",
    "async",
    "await",
    "stylesheets",
    "scripts",
    "images",
    "assets",
    "resources",
    "base64",
    "typescript"
  ],
  "author": "Florian Walzel",
  "license": "MIT",
  "bugs": {
    "url": "https://github.com/fwalzel/async-html-inline/issues"
  },
  "homepage": "https://github.com/fwalzel/async-html-inline#readme",
  "dependencies": {
    "axios": "^1.13.5",
    "esm": "^3.2.25",
    "mime": "^4.1.0",
    "util": "^0.12.5"
  },
  "devDependencies": {
    "@types/node": "^25.2.3",
    "typescript": "^5.9.3"
  }
}
